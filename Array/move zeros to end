 public static void moveZeroes(int arr[]){
        int n=arr.length;
        int count=0;
        for(int i=0; i< n; i++){
            if(arr[i]!=0){
                arr[count++]=arr[i];



            }




        }
        // Make all elements 0 from count to end.
       while (count < n){
           arr[count] = 0;
        count++;}
    }
